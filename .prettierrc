{
    "printWidth": 80,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "tabWidth": 4,
    "semi": true,
    "plugins": [
        "prettier-plugin-organize-imports",
        "prettier-plugin-tailwindcss"
    ]
}
